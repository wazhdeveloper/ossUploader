<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<VBox alignment="CENTER" prefHeight="291.0" prefWidth="417.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jswz.soft.ossuploader.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <TextArea prefHeight="23.0" prefWidth="377.0" promptText="输入想上传的文件地址" scrollLeft="20.0" scrollTop="20.0" fx:id="pathText"/>
    <TextArea prefHeight="12.0" prefWidth="377.0" promptText="输入oss存储的地址(举例: oss://bucket/path/uploaded.txt)" scrollLeft="20.0" scrollTop="20.0" fx:id="urlText"/>
    <Button fx:id="commitButton" onMouseClicked="#onButtonClick" text="commit" />
</VBox>